<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zbkj.crmeb.nsl.crmeb.dao.EbSystemGroupDataMapper">

    <select id="getPublishCourseInfo" resultType="com.zbkj.crmeb.nsl.crmeb.response.EbSystemGroup">
        SELECT id,gid,value,sort,status,create_time,update_time
         FROM `eb_system_group_data`
         where `status` =1 and gid = 48
    </select>
</mapper>
