<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zbkj.crmeb.nsl.nslwxapp.dao.NslCbrandsMapper">

    <select id="getBrandList" resultType="com.zbkj.crmeb.nsl.nslwxapp.model.NslCbrands">
        select id, name,remarks
        from `nsl_cbrands`
        where kbn!='9';
    </select>

</mapper>
