<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zbkj.crmeb.nsl.nslwxapp.dao.NslCollectionMapper">

    <select id="getCraneCollectByUserId" resultType="java.lang.Integer">
        select craneid from `nsl_collection`
        where userid = #{userid} and kbn != 9
        GROUP BY craneid
    </select>
</mapper>
